#include <iostream>
#include <vector>
#include <algorithm>
#include <queue>

using namespace std;

struct cmp {
	bool operator()(int a, int b) {
		return a > b;
	}
};

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	int N,x;
	priority_queue<int,vector<int>, cmp> q;
	cin >> N;
	for (int i = 0; i < N; i++) {
		cin >> x;
		if (x != 0) {
			q.push(x);
		}
		else {
			if (q.empty()) {
				cout << "0\n";
			}
			else {
				cout << q.top() << "\n";
				q.pop();//O(log N)
			}
		}
	}
}//O(N * log N)
