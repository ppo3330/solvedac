//우선순위 큐로 쉽게 풀기
#include <iostream>
#include <vector>
#include <algorithm>
#include <queue>

using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	int N;
	cin >> N;
	vector<int> result;

	priority_queue<int> h;
	for (int i = 0; i < N; i++) {
		int a;
		cin >> a;
		if (a != 0) {
			h.push(a);
		}
		else if (h.empty()) {
			result.push_back(0);
		}
		else {
			result.push_back(h.top());
			h.pop();
		}
	}

	for (int i = 0; i < result.size(); i++) {
		cout << result[i] << "\n";
	}
}
