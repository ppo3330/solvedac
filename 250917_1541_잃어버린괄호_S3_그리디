#include <iostream>
#include <string>

using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	string s;
	bool minus = false;
	cin >> s;
	string num = "";
	int result = 0;

	for (int i = 0; i < s.size(); i++) {
		if (s[i] <= '9' && s[i] >= '0') {
			num += s[i];
			if (i == s.size() - 1) {
				if (minus == false) result += stoi(num);
				else result -= stoi(num);
			}
		}
		else if (s[i] == '-') {
			if (minus == false) result += stoi(num);
			else result -= stoi(num);
			num = "";
			minus = true;
		}
		else if (s[i] == '+') {
			if (minus == false) result += stoi(num);
			else result -= stoi(num);
			num = "";
		}
	}//O(1)
	cout << result;
}
