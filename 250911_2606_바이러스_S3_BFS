#include <iostream>
#include <algorithm>

using namespace std;

int com[101][101];
int ins[101];
int n, link;
int result = 0;

void td(int a) {
	for (int i = 1; i <= n; i++) {
		if (com[a][i] == 1) {
			if (ins[i] == 0) {
				result++;
				ins[i] = 1;
				td(i);
			}
		}
	}
}//n;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);


	cin >> n >> link;

	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= n; j++) {
			com[i][j] = 0;
		}
	}// n^2

	for (int i = 1; i <= n; i++) {
		ins[i] = 0;
	}//n^2

	for (int i = 0; i < link; i++)	{
		int a, b;
		cin >> a >> b;
		com[a][b] = 1;
		com[b][a] = 1;
	}// n
	//연결 완료

	ins[1] = 1;
	td(1);
	cout << result;
	//n^2
}
